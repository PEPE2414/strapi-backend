{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "pluginOptions": {
    "users-permissions": { "user": true }
  },
  "options": { "timestamps": true },
  "attributes": {
    "username": { "type": "string", "minLength": 3, "unique": true, "required": true, "configurable": false },
    "email": { "type": "email", "minLength": 6, "lowercase": true, "unique": true, "required": true, "configurable": false },
    "provider": { "type": "string", "configurable": false },
    "password": { "type": "password", "minLength": 6, "configurable": false, "private": true, "searchable": false },
    "resetPasswordToken": { "type": "string", "configurable": false, "private": true, "searchable": false },
    "confirmationToken": { "type": "string", "configurable": false, "private": true, "searchable": false },
    "confirmed": { "type": "boolean", "default": false, "configurable": false },
    "blocked": { "type": "boolean", "default": false, "configurable": false },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },

    "jobPrefs": { "type": "json", "default": null },
    "preferences": { "type": "json", "default": {} },
    "preferredName": { "type": "string" },
    "university": { "type": "string" },
    "studyField": { "type": "string" },
    "course": { "type": "string" },
    "keyStats": { "type": "json" },
    "plan": { "type": "string" },
    "packages": { "type": "json", "default": [] },
    "addons": { "type": "json", "default": [] },
    "weeklyGoal": { "type": "integer", "default": 12 },
    "notificationPrefs": { "type": "json", "default": {} },
    "deadlineCheckboxes": { "type": "json", "default": {} },
    "deadlineTodos": { "type": "json", "default": {} },
    "skippedPastApps": { "type": "boolean", "default": false },

    "coverLetters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cover-letter.cover-letter",
      "mappedBy": "user"
    },
    "coverLetterPoints": { "type": "json", "default": [] },
    "previousCoverLetterFiles": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["files"]
    },

    "cvFile": { "type": "media", "multiple": false, "allowedTypes": ["files"] },
    "cvText": { "type": "text" },

    "outreachEmails": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::outreach-email.outreach-email",
      "mappedBy": "user"
    },
    "outreachMeta": { "type": "json" },
    "mockInterviewMeta": { "type": "json" },
    "outreachExample": {
      "type": "json",
      "default": {
        "recruiter": { "email": "", "confidence": null, "message": "" },
        "manager": { "email": "", "confidence": null, "message": "" }
      }
    },

    "coverLetterCredits": { "type": "integer", "default": 0 },
    "emailCredits": { "type": "integer", "default": 0 },
    "interviewQuestionCredits": { "type": "integer", "default": 0 },
    "mockInterviewMinutes": { "type": "integer", "default": 0 },

    "usageLogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::usage-log.usage-log",
      "mappedBy": "user"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "user"
    },

    "profilePicture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "avatarChoice": {
      "type": "string"
    },
    "reviewSubmitted": {
      "type": "boolean",
      "default": false
    },
    "reviewPromptDismissedAt": {
      "type": "datetime"
    },
    "featureUsageCount": {
      "type": "json",
      "default": {
        "coverLetters": 0,
        "recruiterLookups": 0,
        "mockInterviews": 0,
        "interviewQuestions": 0,
        "cheatSheets": 0,
        "warmUps": 0
      }
    },

    "referralCode": {
      "type": "string",
      "unique": true,
      "minLength": 4,
      "maxLength": 8
    },
    "referredBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "referrals"
    },
    "referrals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "referredBy"
    },
    "promoCode": {
      "type": "string",
      "unique": true,
      "minLength": 10,
      "maxLength": 20
    },
    "promoCodeId": {
      "type": "string",
      "private": true
    },
    "qualifiedReferrals": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "referralRewards": {
      "type": "json",
      "default": []
    },
    "guaranteeActive": {
      "type": "boolean",
      "default": false
    },
    "fastTrackUntil": {
      "type": "datetime"
    }
  }
}
