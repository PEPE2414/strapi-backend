  },
  "pluginOptions": {},
  "attributes": {
    "sessionId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "userId": {
      "type": "string",
      "required": true
    },
    "jobId": {
      "type": "string"
    },
    "jobTitle": {
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "settings": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "active", "completed", "failed"],
      "default": "pending"
    }
  }
