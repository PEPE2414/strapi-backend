{
  "kind": "collectionType",
  "collectionName": "mock_interviews",
  "info": {
    "singularName": "mock-interview",
    "pluralName": "mock-interviews",
    "displayName": "Mock Interview",
    "description": "Mock interview sessions and configuration"
  },
