{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",
    "jobs:ingest": "tsx jobs-ingest/src/index.ts",
    "jobs:test": "tsx jobs-ingest/src/test.ts",
    "jobs:build": "tsc -p jobs-ingest/tsconfig.json",
    "jobs:cleanup": "tsx -e \"import('./src/services/jobCleanup').then(m => m.jobCleanupService.cleanup()).then(console.log).catch(console.error)\"",
    "jobs:cleanup-stats": "tsx -e \"import('./src/services/jobCleanup').then(m => m.jobCleanupService.getStats()).then(console.log).catch(console.error)\"",
    "jobs:remove-duplicates": "node jobs-ingest/remove-duplicates.js",
    "jobs:remove-duplicates-robust": "node jobs-ingest/remove-duplicates-robust.js",
    "referrals:generate": "tsx -e \"import('./src/scripts/generate-referral-codes').then(m => m.default()).then(() => console.log('Done')).catch(console.error)\""
  },
  "dependencies": {
    "@strapi/admin": "5.21.0",
    "@strapi/data-transfer": "5.21.0",
    "@strapi/plugin-cloud": "5.21.0",
    "@strapi/plugin-users-permissions": "5.21.0",
    "@strapi/provider-upload-aws-s3": "^5.23.4",
    "@strapi/strapi": "5.21.0",
    "better-sqlite3": "11.3.0",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.15.1",
    "mammoth": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "playwright": "^1.40.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "robots-parser": "^3.0.1",
    "sqlite3": "^5.1.7",
    "stripe": "^19.1.0",
    "styled-components": "^6.1.19",
    "undici": "^6.19.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsx": "^4.16.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18 <23",
    "npm": ">=6"
  },
  "strapi": {
